{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "baseUrl": "../",
    "resolveJsonModule": true,
    "paths": {
      "common/*": ["src/common/*"],
      "PlatformDelegate": ["src/electron/Delegates.ts", "src/web/Delegates.ts"],
      "platform/*": ["src/electron/platform/*", "src/web/platform/*"]
    }
  }
}
