{
  "compilerOptions": {
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "sourceMap": true,
    "allowJs": false,
    "esModuleInterop": true,
    "jsx": "react",
    "downlevelIteration": true,
    "lib": ["dom"],
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "common/*": ["src/common/*"],
      "PlatformDelegate": ["src/electron/Delegates.ts", "src/web/Delegates.ts"],
      "Components": ["src/electron/Components.ts", "src/web/Components.ts"]
    }
  }
}
