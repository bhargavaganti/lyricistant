{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "sourceMap": true,
    "allowJs": false,
    "esModuleInterop": true,
    "jsx": "react",
    "downlevelIteration": true,
    "lib": ["esnext", "dom"],
    "baseUrl": "./",
    "paths": {
      "common/*": ["src/common/*"],
      "PlatformDelegate": ["src/electron/Delegates.ts", "src/web/Delegates.ts"],
      "platform/*": ["src/electron/platform/*", "src/web/platform/*"]
    }
  }
}
