{
  "imports": {
    "PreferencesData": "./preferences/PreferencesData",
    "UiConfig": "./ui/UiConfig"
  },
  "rendererChannels": [
    {
      "channel": "dark-mode-toggled",
      "textSize": "number | null",
      "useDarkColors": "boolean"
    },
    {
      "channel": "prefs-updated",
      "preferenceData": "PreferencesData"
    },
    { "channel": "new-file-created" },
    {
      "channel": "file-save-ended",
      "error": "Error | null",
      "currentFilePath": "string"
    },
    { "channel": "close-prefs" },
    { "channel": "find" },
    { "channel": "replace" },
    { "channel": "is-okay-for-new-file" },
    {
      "channel": "file-opened",
      "error": "Error",
      "filePath": "string",
      "data": "string"
    },
    {
      "channel": "file-save-started",
      "filePath": "string"
    },
    { "channel": "request-editor-text" },
    { "channel": "is-okay-for-quit-file" },
    { "channel": "undo" },
    { "channel": "redo" },
    { "channel": "open-prefs" },
    {
      "channel":  "ui-config",
      "config":  "UiConfig"
    },
    {
      "channel": "app-title-changed",
      "title": "string"
    }
  ],
  "platformChannels": [
    { "channel": "ready-for-events" },
    {
      "channel": "editor-text",
      "text": "string"
    },
    { "channel": "prompt-save-file-for-new" },
    { "channel": "prompt-save-file-for-quit" },
    { "channel": "okay-for-new-file" },
    { "channel": "okay-for-quit" },
    {
      "channel": "save-prefs",
      "data?": "PreferencesData"
    },
    { "channel": "new-file-attempt" },
    { "channel": "open-file-attempt" },
    { "channel": "save-file-attempt", "data": "string" },
    { "channel": "request-ui-config" }
  ]
}
